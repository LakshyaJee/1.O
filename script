<script>
    document.addEventListener('DOMContentLoaded', function() {
    const videoId = "649c3e39da2c7b00184a4114"; // Get videoId from PHP
    const videoTimestamps = JSON.parse(getCookie('videoTimestamps') || '{}');
    
    if (videoTimestamps[videoId] && videoTimestamps[videoId].completion) {
        const completionPercentage = videoTimestamps[videoId].completion;
        const progressBar = document.getElementById('progress-bar-' + videoId);
        
        if (progressBar) {
            progressBar.style.width = completionPercentage + '%';
        }
    }
});

function getCookie(name) {
    const decodedCookie = decodeURIComponent(document.cookie);
    const ca = decodedCookie.split(';');
    for (let i = 0; i < ca.length; i++) {
        let c = ca[i].trim();
        if (c.indexOf(name + "=") === 0) {
            return c.substring(name.length + 1);
        }
    }
    return "";
}

</script>
