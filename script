<script>
// Function to get a cookie by name
function getCookie(name) {
    let match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
    if (match) {
        return match[2].split(',');
    } else {
        return [];
    }
}

// Function to set a cookie by name
function setCookie(name, value, days) {
    let expires = "";
    if (days) {
        let date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + (value || "") + expires + "; path=/";
}

$(document).ready(function() {
    const completedVideos = getCookie('completedVideos');

    // Check cookies on page load
    completedVideos.forEach(videoId => {
        const statusElement = $('#status-' + videoId);
        if (statusElement.length) {
            statusElement.show();
            $('#dropdown-' + videoId).remove(); // Hide dropdown
        }
    });

    // Custom dropdown logic
    $('.custom-dropdown-toggle').click(function(event) {
        event.preventDefault();
        const dropdown = $(this).closest('.custom-dropdown');
        dropdown.toggleClass('open');
        $('.custom-dropdown').not(dropdown).removeClass('open'); // Close other dropdowns
    });

    // Close dropdown when clicking outside
    $(document).click(function(event) {
        if (!$(event.target).closest('.custom-dropdown').length) {
            $('.custom-dropdown').removeClass('open');
        }
    });

    // Mark as Complete
    $('.mark-complete').click(function(event) {
        event.preventDefault();
        const videoId = $(this).data('id');
        
        // Add video ID to cookies
        let completedVideos = getCookie('completedVideos');
        if (!completedVideos.includes(videoId.toString())) {
            completedVideos.push(videoId);
            setCookie('completedVideos', completedVideos.join(','), 365);

            // Update UI
            $('#status-' + videoId).show();
            $('#dropdown-' + videoId).remove(); // Hide dropdown
        }
    });

    // Play Video with CORS option
    // $('.video-link').click(function(event) {
    //     event.preventDefault();
    //     const redirectUrl = $(this).data('url');
    //     Swal.fire({
    //         title: 'How would you like to play this video?',
    //         text: 'Choose an option below:',
    //         icon: 'question',
    //         showDenyButton: true,
    //         showCancelButton: true,
    //         confirmButtonText: 'Play with CORS',
    //         denyButtonText: 'Play without CORS',
    //     }).then((result) => {
    //         if (result.isConfirmed) {
    //             window.location.href = redirectUrl + '&cors=1'; /// with cors extension
    //         } else if (result.isDenied) {
    //             window.location.href = redirectUrl + '&cors=0';
    //         }
    //     });
    // });
});
</script>

