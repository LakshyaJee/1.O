    <script>
  function formatTotalFeeInRupeesWithCommas(fee) {
        var formattedFee = new Intl.NumberFormat('en-IN', {
            style: 'currency',
            currency: 'INR',
            maximumFractionDigits: 2
        }).format(fee);

        return formattedFee.replace('INR');
    }

    function updateTotalFee() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', 'get_total_fee.php', true);
        xhr.onreadystatechange = function () {
            if (xhr.readyState == 4 && xhr.status == 200) {
                var newFee = parseFloat(xhr.responseText);
                var totalFeeElement = document.getElementById('totalFee');
                var totalFeeAlertElement = document.getElementById('totalFeeAlert');

                animateTotalFee(totalFeeElement, currentFee, newFee);
                currentFee = newFee;

                totalFeeAlertElement.style.display = 'block';
            } else if (xhr.readyState == 4 && xhr.status != 200) {
                console.log('Error fetching total fee');
            }
        };
        xhr.send();
    }

    function animateTotalFee(element, start, end) {
        var duration = 2000; // Change the duration as needed
        var startTime = null;

        function step(timestamp) {
            if (!startTime) startTime = timestamp;
            var progress = timestamp - startTime;

            element.textContent = formatTotalFeeInRupeesWithCommas(
                start + (end - start) * (progress / duration)
            );

            if (progress < duration) {
                requestAnimationFrame(step);
            } else {
                element.textContent = formatTotalFeeInRupeesWithCommas(end);
            }
        }

        requestAnimationFrame(step);
    }

    var currentFee = 0;
    updateTotalFee();

    setInterval(function () {
        updateTotalFee();
    }, 120000);
    
     /**
   * Preloader
   */
  let preloader = select('#preloader');
  if (preloader) {
    window.addEventListener('load', () => {
      preloader.remove()
    });
  }

</script>


